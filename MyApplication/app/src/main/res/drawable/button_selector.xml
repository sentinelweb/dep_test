<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_enabled="false" android:drawable="@color/disabled"/>
	<item android:state_pressed="true" android:drawable="@color/primary"/>
	<item android:state_selected="true" android:drawable="@color/selected"/>
    <item android:state_checked="true" android:drawable="@color/checked"/>
    <item android:drawable="@color/transparent"/>
</selector> 
